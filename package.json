{
  "name": "blocks-redux",
  "version": "1.0.0",
  "description": "Self building redux actions and reducers",
  "repository": "https://github.com/bl-brandon/blocks-redux",
  "scripts": {
    "build": "npm run clean && npm run compile && npm run package",
    "clean": "rimraf dist",
    "compile": "babel ./src --out-dir ./build --ignore spec.js",
    "package": "babel-node ./bin/createPackage.js",
    "publish": "npm run build && cd ./build && npm publish"
  },
  "license": "MIT",
  "devDependencies": {
    "chalk": "^1.1.3",
    "fs-extra": "^2.1.2",
    "rimraf": "^2.5.4"
  },
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.5.1",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "isomorphic-fetch": "^2.2.1",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.2.0"
  }
}
